<a href="<%= project_path(@conn, :new) %>">New Project</a>
<ul>
  <%= for project <- @projects do %>
    <li>
    <%= project.name %>
    <a href="<%= project_path(@conn, :edit, project.id) %>" class="btn btn-default">Edit</a>
    <form action="<%= project_path(@conn, :destroy, project.id) %>" method="post">
      <button type="submit" class="btn btn-danger">Delete</button>
    </form>
    </li>
  <% end %>
</ul>
